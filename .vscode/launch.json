    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "http://localhost:8080",
            "webRoot": "pamled/pamled_editor/client/src",
               "breakOnLoad": true,
               "sourceMapPathOverrides": {
               "webpack:///src/*": "${webRoot}/*"
            }
       },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pamled/manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        }
    ]
}